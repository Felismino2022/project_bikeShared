<button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#estacao" data-bs-whatever="@mdo">Levantar</button>

 <div class="modal fade" id="estacao" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <h1 class="modal-title fs-5" id="estacao">Adicionar Estacão</h1>
              <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
              <form action="/estacao" method="post">
                <div class="mb-3">
                    <label for="recipient-name" class="col-form-label">numero:</label>
                    <input type="number" class="form-control" id="recipient-name" name="num">
                  </div>
                  <div class="mb-3">
                    <label for="recipient-name" class="col-form-label">Latitude:</label>
                    <input type="number" class="form-control" id="recipient-name" name="latitute">
                  </div>

                  <div class="mb-3">
                    <label for="recipient-name" class="col-form-label">Longuitude:</label>
                    <input type="number" class="form-control" id="recipient-name" name="longuitude">
                  </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Fechar</button>
                    <button type="submit" class="btn btn-primary">Enviar</button>
                </div>
              </form>
            </div>
           
          </div>
        </div>
      </div>

      <h2>Estações:</h2>
      {{#each vetores}}
        <h3>nome: {{nome}}</h3>
      {{else}}
      <h5>Nenhuma Estação</h5>
      {{/each}}